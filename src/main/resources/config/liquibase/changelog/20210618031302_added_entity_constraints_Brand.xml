<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.3.xsd">
    <!--
        Added the constraints for entity Brand.
    -->
    <changeSet id="20210618031302-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="companyof_medicine_id"
                                 baseTableName="brand"
                                 constraintName="fk_brand__companyof_medicine_id"
                                 referencedColumnNames="id"
                                 referencedTableName="company"/>

        <addForeignKeyConstraint baseColumnNames="brand_id"
                                 baseTableName="rel_brand__generics"
                                 constraintName="fk_rel_brand__generics__brand_id"
                                 referencedColumnNames="id"
                                 referencedTableName="brand"/>

        <addForeignKeyConstraint baseColumnNames="generics_id"
                                 baseTableName="rel_brand__generics"
                                 constraintName="fk_rel_brand__generics__generics_id"
                                 referencedColumnNames="id"
                                 referencedTableName="generics"/>
    </changeSet>
</databaseChangeLog>
